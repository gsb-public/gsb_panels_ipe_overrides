<?php

/**
 * Implementation of hook_ctools_content_subtype_alter()
 *
 */
function gsb_panels_ipe_overrides_ctools_content_subtype_alter(&$subtype, &$plugin) {
  if (isset($subtype['name'])) {
    switch ($subtype['name']) {
      case 'accordion':
        $subtype['category'] = 'GSB';
        $subtype['title'] = 'Accordion';
        $subtype['top level'] = FALSE;
        break;

      default:
        break;
    }
  }
}

